{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
      "framework",
      "laravel"
  ],
  "license": "MIT",
  "require": {
      "php": "^8.1",
      "fideloper/proxy": "^4.2",
      "fruitcake/laravel-cors": "^2.0",
      "guzzlehttp/guzzle": "^7.0.1",
      "laravel/framework": "^8.0",
      "laravel/tinker": "^2.0",
      "predis/predis": "^1.1",
      "spatie/laravel-server-side-rendering": "^1.4",
      "webfitters/fittingroom": "dev-dev/auth-rework",
      "webfitters/store": "dev-master",
      "webfitters/store-tax": "dev-master",
      "webfitters/store-discounts": "dev-master",
      "webfitters/store-payments-elavon": "dev-master",
      "webfitters/store-payments-purchaseorder": "dev-master",
      "webfitters/store-shipping": "dev-master",
      "webfitters/store-shipping-ups": "dev-master",
      "webfitters/store-shipping-flatrate": "dev-master",
      "webfitters/store-shipping-collect": "dev-master",
      "webfitters/store-businessedge": "dev-master",
      "webfitters/store-larsen-custom": "dev-master"
  },
  "require-dev": {
      "facade/ignition": "^2.3.6",
      "fzaninotto/faker": "^1.9.1",
      "mockery/mockery": "^1.3.1",
      "nunomaduro/collision": "^5.0",
      "phpunit/phpunit": "^9.3"
  },
  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true,
      "gitlab-domains": [
          "gitlab.webfitters.com"
      ]
  },
  "extra": {
      "laravel": {
          "dont-discover": []
      }
  },
  "autoload": {
      "psr-4": {
          "App\\": "app/",
          "Database\\Factories\\": "database/factories/",
          "Database\\Seeders\\": "database/seeders/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
      "post-autoload-dump": [
          "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
          "@php artisan package:discover --ansi",
          "@php artisan store:install",
          "@php artisan store:cache:flush"
      ],
      "post-update-cmd": [
          "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
          "composer check-platform-reqs",
          "echo consider runing composer bump"
      ],
      "post-create-project-cmd": [
          "@php artisan key:generate --ansi"
      ]
  },
  "repositories": [
      {
          "type": "composer",
          "url": "https://gitlab.webfitters.com/api/v4/group/46/-/packages/composer/"
      }
  ]
}
